{
	"name": "satisfactory-fabriken",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build --mode production",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
